algoritmo "DivGit"

var
  V1,V2, S:real

Funcao Divisao(X,Y:real):real
       Retorne X/Y
FimFuncao
inicio
      Escreva("Digite o primeiro valor: ")
      Leia(V1)
      Escreva("Digite o segundo valor: ")
      Leia(V2)
      S <- Divisao(V1, V2)
      Escreva("A Divisao entre ",V1," e ",V2," igual a: ",S)
      

fimalgoritmo
